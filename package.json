{
  "name": "testcafe-typescript-framework",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "pretest": "npm run build",
    "test": "testcafe chrome src/tests/**/*.ts --reporter spec,html:reports/test-report.html --screenshots path=reports/screenshots/$(date +%Y-%m-%d_%H-%M-%S),takeOnFails=true",
    "test_github_actions": "testcafe 'chrome:headless' src/tests/**/*.ts --reporter list,html:reports/test-report.html --screenshots path=reports/screenshots/$(date +%Y-%m-%d_%H-%M-%S),takeOnFails=true",
    "watch": "tsc --watch",
    "dev": "concurrently \"npm run watch\" \"npm run test\""
  },
  "keywords": [],
  "author": "",
  "description": "",
  "dependencies": {
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "testcafe-reporter-spec": "^2.2.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "testcafe": "^3.6.1",
    "testcafe-reporter-html": "^1.4.6",
    "typescript": "^5.5.2"
  }
}
